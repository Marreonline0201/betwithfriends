{
  "name": "bet-tracker",
  "version": "1.0.0",
  "description": "A website for couples/friends to track bets and wins in games",
  "scripts": {
    "server": "node server/index.js",
    "client": "cd client && npm start",
    "build": "bash build.sh",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "setup": "npm install && cd client && npm install"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "better-sqlite3": "^11.6.0",
    "cors": "^2.8.5",
    "express": "^4.21.1",
    "jsonwebtoken": "^9.0.2"
  },
  "devDependencies": {
    "concurrently": "^9.1.0"
  }
}
